            print(" - " .. v.Name .. " (" .. v.ClassName .. ")")
        end
        print("ServerStorage Contents:")
        for _, v in pairs(ss:GetChildren()) do
            print(" - " .. v.Name .. " (" .. v.ClassName .. ")")
        end
    end
})

